  seq:chr      "<%= rec.chrom %>" ;
  seq:pos      <%= rec.pos %> ;
  seq:ref      "<%= rec.ref %>" ;
  seq:alt      "<%= rec.alt[0] %>" ;
  seq:dp       <%= rec.info.dp %> ;
  db:vcf       true .

